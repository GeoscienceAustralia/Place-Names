@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix pn:  <https://linked.data.gov.au/def/placenames/> .
@prefix :  <https://linked.data.gov.au/def/placenames/shapes/> .


:PlacenameShape
    a sh:NodeShape ;
    sh:deactivated false;
    sh:targetClass pn:PlaceName;
    sh:property [
        sh:path pn:hasName ;
        sh:datatype xsd:string ;
        sh:name "has name" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        rdfs:comment "Must have one and only one 'has name' data type property (string)"
    ] .

