#################################################################

###  http://linked.data.gov.au/def/placenames/hasPronunciation
:hasPronunciation rdf:type owl:DatatypeProperty ;
                  rdfs:domain :PlaceName ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The pronunciation of a Place Name, indicated by means of a phonetic alphabet string"@en ;
                  rdfs:label "pronunciation"@en .


###  http://linked.data.gov.au/def/placenames/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:label "name" .


#################################################################
#    Classes
#################################################################

###  http://linked.data.gov.au/def/placenames/Gazetteer
:Gazetteer rdf:type owl:Class ;
           rdfs:subClassOf rdf:Bag ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty rdfs:member ;
                             owl:allValuesFrom :PlaceName
                           ] ;
           rdfs:comment "A Gazetteer is a list of Place Names"@en ;
           rdfs:label "Gazetteer"@en ;
           skos:changeNote """
was- 
  	rdfs:subClassOf reg:Register ;
  	rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue :PlaceName ;
      owl:onProperty reg:containedItemClass ;
    ] ;
But this is not appropriate - according to the Registry Ontology a reg:Register is a set of metadata records, not a set of Entities. 

Use rdf:Bag and rdfs:member instead. 
""" .


###  http://linked.data.gov.au/def/placenames/Jurisdiction
:Jurisdiction rdf:type owl:Class ;
              rdfs:subClassOf dcterms:Agent ;
              rdfs:comment "Local, State, Territory or National (Commonwealth) Government that has authority to formally establish (i.e. gazette) place names within its jurisdictional area."@en ;
              rdfs:label "Jurisdiction" .


###  http://linked.data.gov.au/def/placenames/Place
:Place rdf:type owl:Class ;
       rdfs:subClassOf geo:Feature ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasPlaceClassification ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty geo:hasGeometry ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "An identifiable geographic Place as defined by the Composite Gazetteer of Australia. A Place is classified according to an extensible set of types."@en ;
       rdfs:label "Place"@en .


###  http://linked.data.gov.au/def/placenames/PlaceName
:PlaceName rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty <http://linked.data.gov.au/def/loci#isMemberOf> ;
                             owl:someValuesFrom :Gazetteer
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :status ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty dcterms:identifier ;
                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :name ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "The name of a place, assigned by an official naming authority and included in the Place Names Gazeteer of Australia."@en ;
           rdfs:label "Place Name"@en ;
           skos:changeNote "Removed subClassOf reg:RegisterItem axiom, because reg:RegisterItem is \"A metadata record for an entry in a register. \" and is not an Entity itself. " .


###  http://linked.data.gov.au/def/placenames/PlaceNameFormality
:PlaceNameFormality rdf:type owl:Class ;
                    rdfs:subClassOf skos:Concept ;
                    rdfs:comment "The formality of a Place Name: official, historical, informal"@en ;
                    rdfs:label "Place Name Formality"@en .


###  http://linked.data.gov.au/def/placenames/PlaceType
:PlaceType rdf:type owl:Class ;
           rdfs:subClassOf skos:Concept ;
           rdfs:comment "The classification of a place, from the ICSM Permanent Committee on Place Names classification scheme."@en ;
           rdfs:label "Place Type"@en .


###  http://linked.data.gov.au/def/placenames/PointOfInterest
:PointOfInterest rdf:type owl:Class ;
                 rdfs:subClassOf :Place ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :wasNamedBy ;
                                   owl:allValuesFrom dcterms:Agent
                                 ] ;
                 rdfs:comment "A specific point Location that someone may find useful or interesting."@en ;
                 rdfs:label "Point of Interest"@en .


###  http://linked.data.gov.au/def/placenames/Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf :Place ;
        rdfs:comment "A Region is an area of land that has common features."@en ;
        rdfs:label "Region"@en .


###  http://linked.data.gov.au/def/placenames/Status
:Status rdf:type owl:Class ;
        rdfs:subClassOf skos:Concept ;
        rdfs:label "Status " .


